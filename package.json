{
  "name": "picidae-backstage-system",
  "version": "1.0.0",
  "private": true,
  "description": "[![NPM version][npm-image]][npm-url] [![build status][travis-image]][travis-url] [![Test coverage][coveralls-image]][coveralls-url] [![Dependency Status][depstat-image]][depstat-url] [![Dev Dependency Status][devdepstat-image]][devdepstat-url]",
  "main": "dist/backend/index.js",
  "scripts": {
    "start": "node .",
    "nodemon:debug": "npm run nodemon -- --inspect",
    "nodemon": "nodemon --signal SIGINT -w dist/backend ./dist/backend",
    "postinstall": "npm run back:build; ln -sf \"$PWD\"\"/scripts/pre-commit\" .git/hooks",
    "test": "mocha dist/backend/test/** -R list",
    "test:web": "mocha dist/backend/test/web/** -R list",
    "test:lib": "mocha dist/backend/test/lib/** -R list",
    "babel:build": "rm -rf dist/* && mkdir -p dist && babel src -d dist --ignore=src/frontend/**.js* -D",
    "dev": "npm run back:dev & npm run front:dev & npm run nodemon:debug",
    "back:dev": "npm run back:build -- -w",
    "back:build": "rm -rf dist/{backend,node_modules} && babel src -d dist --ignore=src/frontend/** -D",
    "front:dev": "cd src/frontend && cross-env NODE_ENV=development BABEL_ENV=browser webpack-dev-server --progress",
    "front:build": "rm -rf dist/frontend/*; cd src/frontend && cross-env NODE_ENV=production BABEL_ENV=browser webpack ",
    "lint": "eslint ./src --fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
